{
  "$schema": "https://opencode.ai/config.json",
  "theme": "gruvbox",
  "share": "disabled",
  "autoupdate": false,
  "keybinds": {
    "leader": "ctrl+space"
  },
  "provider": {
    "openai-compatible": {
      "name": "Skyscanner ModelOps (Claude)",
      "npm": "@ai-sdk/openai-compatible",
      "api": "{env:PORTKEY_URL}",
      "options": {
        "apiKey": "dummy",
        "baseURL": "{env:PORTKEY_URL}",
        "headers": {
          "x-portkey-api-key": "{env:PORTKEY_API_KEY}",
          "x-portkey-config": "{env:PORTKEY_CONFIG_ID}"
