          "tool_call": true,
          "attachment": true,
          "temperature": true,
          "reasoning": false,
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "cost": {
